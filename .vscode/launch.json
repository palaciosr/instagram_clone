{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI Debug",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--reload"
      ],
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/instagram_clone/app" //workspaceFolder
      },
      "envFile": "${workspaceFolder}/instagram_clone/app/.env",
      "console": "integratedTerminal"
    }
  ]
}   